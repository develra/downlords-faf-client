<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="playRoot" maxHeight="1.7976931348623157E308"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.faforever.client.play.PlayController">
    <TabPane fx:id="playRootTabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
             tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <Tab fx:id="customGamesTab" text="%play.custom">
            <fx:include fx:id="customGames" source="custom_games.fxml"/>
        </Tab>
        <Tab fx:id="ladderTab" text="%play.ranked1v1">
            <fx:include fx:id="ladder" source="ranked_1v1.fxml"/>
        </Tab>
        <Tab fx:id="coopTab" text="%play.coop">
            <fx:include fx:id="coop" source="coop/coop.fxml"/>
        </Tab>
    </TabPane>
</VBox>
